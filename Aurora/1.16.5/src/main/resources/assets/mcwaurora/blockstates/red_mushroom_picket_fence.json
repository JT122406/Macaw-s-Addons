{
  "multipart": [
    {
      "apply": {
        "model": "mcwaurora:block/horse/red_mushroom_log_fence_post"
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "mcwaurora:block/picket/red_mushroom_picket_fence_side",
        "uvlock": false
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "mcwaurora:block/picket/red_mushroom_picket_fence_side",
        "y": 90,
        "uvlock": false
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "mcwaurora:block/picket/red_mushroom_picket_fence_side",
        "y": 180,
        "uvlock": false
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "mcwaurora:block/picket/red_mushroom_picket_fence_side",
        "y": 270,
        "uvlock": false
      }
    }
  ]
}
